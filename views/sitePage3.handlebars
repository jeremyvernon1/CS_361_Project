{{!-- Flexbox container to setup 3 boxes --}}
<div class="flexbox-container">
    
    {{!-- Airport 1 --}}
    <div class="flexbox-item">
        <h3 class="center">Airport 1</h3>
        {{!-- Grid container within flexbox --}}
        <div class="grid-container">
            
            {{!-- First row --}}
            {{!-- Stats --}}
            <div class="grid-item grid-inner-container">
                
                {{!-- Location --}}
                <div> Location: </div>
                <div> <span class="location-value"></span> </div>
                
                {{!-- GPS --}}
                <div> GPS: </div>
                <div> <span class="gps-value"></span> </div>

                {{!-- Elevation --}}
                <div> Elevation: </div>
                <div> <span class="elevation-value"></span> </div>

                {{!-- Landing Fee --}}
                <div> Landing Fee: </div>
                <div>
                    <span class="landing-fee-value"></span> 
                    <button>Convert</button>
                </div>

                {{!-- Traffic --}}
                <div> Traffic: </div>
                <div> <span class="traffic-value"></span> </div>

           </div>
            
            {{!-- Photo --}}
            <div class="grid-item">
                <span class="photo-1"></span>
           </div>

            {{!-- Second row --}}
            <div class="grid-full-column">
                Amenities
           </div>

            {{!-- Third row --}}            
            {{!-- Distance 1 --}}
            <div class="grid-item no-bottom-border no-right-border">
                Distance from Airport 1 to Airport 2:
           </div>
            <div class="grid-item no-bottom-border no-left-border">
                <span class="distance-value-1"></span> miles
           </div>
            
            {{!-- Distance 2 --}}
            <div class="grid-item no-top-border no-right-border">
                Distance from Airport 1 to Airport 3:
           </div>
            <div class="grid-item no-top-border no-left-border">
                <span class="distance-value-2"></span> miles
           </div>

            {{!-- Fourth row --}}
            <div class="grid-full-column">
                <span class="airport-info-1"></span>
           </div>
        {{!-- End grid container within flexbox --}}
       </div>
    {{!-- End Airport 1 --}}
